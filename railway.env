# Railway Environment Variables
PORT=8001
# DATABASE_URL will be automatically provided by Railway when you add a PostgreSQL service
# Remove the hardcoded DATABASE_URL below and use Railway's provided one
# DATABASE_URL=postgresql://postgres:mysecretpassword@postgres-container:5432/mommyshops_db
NVIDIA_API_KEY=your-nvidia-api-key-here

# Database Configuration - These will be overridden by Railway's DATABASE_URL
# DB_HOST=postgres-container
# DB_PORT=5432
# DB_NAME=mommyshops_db
# DB_USER=postgres
# DB_PASSWORD=mysecretpassword

# API Configuration
API_TIMEOUT=60
MAX_FILE_SIZE=5242880

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=https://web-production-f23a5.up.railway.app/auth/google/callback
RAILWAY_ENVIRONMENT=true